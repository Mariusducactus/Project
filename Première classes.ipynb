    "function dt!(balle::Balle, partie::Partie)\n",
    "    xp= balle.x+dt*v*cos(theta)\n",
    "    yp= balle.y+dt*v*sin(theta)\n",
    "    \n",
    "    deplacement = LineSegment([x,y], [xp,yp])\n",
    "    for i in Partie.obstacles\n",
    "        if intersect(deplacement, i)\n",
    "            partie.is_over=true\n",
    "        end\n",
    "    end\n",
    "    if !partie.is_over\n",
    "        balle.x=xp\n",
    "        balle.y=yp\n",
    "        balle.v+=dt*f/m\n",
    "    end\n",
    "end"
   ]
